package com.deitel.deitalpractice;

public class Car {

	private String model;
	private String year;
	private double price;
	
	public Car(String model, String year, double price) {
		this.model = model;
		this.year = year;
		this.price = price;
	}
	
	//setter methods
	public void setModel(String model) {
		this.model = model;
	}
	
	public void setYear(String year) {
		this.year = year;
	}

}
