package com.deitel.deitalpractice;

public class SelfReview_4_6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int product = 1;
		int a = 1;
		product *= a;
		while(a < 6) {
			product *= a;
			a++;
		}
		
		System.out.printf("The product is: %d", product);
	}

}
