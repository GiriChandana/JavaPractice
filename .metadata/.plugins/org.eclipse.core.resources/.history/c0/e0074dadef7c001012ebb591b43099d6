package com.deitel.deitalpractice;

import java.util.Scanner;

public class Exercise_4_20 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input = new Scanner(System.in);
		int numberOfCitizens = 5;
		int counter = 0;
		while(numberOfCitizens > 0) {
			System.out.printf("Enter the earning of citizen %d", counter + 1);
			int earnings = input.nextInt();
			numberOfCitizens--;
			counter++;
		}
	}

}
